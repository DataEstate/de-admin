[ignore]
./node_modules/module-deps/.*
./jest/.*

[include]
./node_modules
./src/**/*

[libs]

[lints]
sketchy-null=warn
untyped-import=warn

[options]
module.file_ext=.js
module.file_ext=.jsx
module.system.node.resolve_dirname=node_modules
module.system.node.resolve_dirname=src
suppress_type=$DownCast
suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe
